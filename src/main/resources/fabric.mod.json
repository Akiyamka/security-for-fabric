  "name": "Security For Fabric",
  "description": "Security4Fabric is a mod that includes several stuff for you to keep your house save.",
  "authors": [
    "Mindsh4ke | Albii[YT]"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/securityforfabric/icon.png",

  "environment": "*",
  "entrypoints": {
